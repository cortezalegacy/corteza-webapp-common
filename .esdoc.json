{
  "source": "./src",
  "destination": "./docs",
  "plugins": [
    {"name": "esdoc-ecmascript-proposal-plugin", "option": {"all": true}},
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "accessor": {
          "access": ["public"]
        },
        "manual": {
          "files": [
            "./manual/automation-scripts/README.md",
            "./manual/automation-scripts/filtering.md"
          ]
        }
      }
    }
  ]
}
